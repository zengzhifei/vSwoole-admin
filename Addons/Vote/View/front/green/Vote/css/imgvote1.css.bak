.vote-list ul{padding:10px 10px 0}.vote-list .vote-item{display:block;padding:0 0 10px}.vote-item .item-link{display:block;height:143px;border:1px solid #ccc;-webkit-box-sizing:border-box}.vote-item .item-link img{height:100%;width:100%}.vote-item .item-desc{font-size:12px}.vote-item .item-desc p{display:-webkit-box;height:40px;line-height:40px;-webkit-line-clamp:1;-webkit-box-orient:vertical;overflow:hidden;color:#212121}.vote-item .item-desc .item-result{display:-webkit-box;margin:10px 0 0;-webkit-box-align:center;-webkit-box-pack:Justify}.vote-item .item-desc .item-bar-wrap{display:-webkit-box;width:80.8%;-webkit-box-align:center}.vote-item .item-desc .item-bar-inner{position:relative;height:6px;border-radius:6px;background-color:#ebebeb;-webkit-box-flex:1;-webkit-box-shadow:1px 1px 1px 1px #d3d3d3 inset}.vote-item .item-bar-inner .item-bar{height:6px;border-radius:6px;background-color:#30ff00}.vote-item .item-bar-wrap .item-percent{margin-left:2px;font-size:9px;color:#30ff00;line-height:16px}.vote-item-first .item-bar-inner .item-bar{background-color:#ff7800}.vote-item-first .item-bar-wrap .item-percent{color:#ff7800}.vote-item-second .item-bar-inner .item-bar{background-color:#fc0}.vote-item-second .item-bar-wrap .item-percent{color:#fc0}.vote-item .item-desc .item-num{text-align:center;font-size:9px;line-height:9px;color:#333}.vote-item .item-btn{display:block;background-color:#f5f5f5}.btn-icon{float:left;width:20px;height:20px;margin:10px 10px 0 0;background:url(../../../img/web_bb/vote/vote-btn.png) 0 0/176px 22px no-repeat}.vote-item .item-btn .icon-radio{background-position:-88px 0}.vote-item .item-selected .icon-radio{background-position:-110px 0}.vote-item .item-btn .icon-checkbox{background-position:0 0}.vote-item .item-selected .icon-checkbox{background-position:-22px 0}.multi-vote-list .num-max{color:#ff2400}